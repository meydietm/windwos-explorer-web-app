# Database (PostgreSQL) - used by Docker Compose and the API

POSTGRES_USER=explorer
POSTGRES_PASSWORD=explorer
POSTGRES_DB=explorer
POSTGRES_PORT=5432

# Use this in the API (Drizzle, etc.)
DATABASE_URL=postgresql://explorer:explorer@localhost:5432/explorer

# API
API_PORT=3001

# CORS (optional)
# CORS_ORIGIN=http://localhost:5173

# WEB
VITE_TREE_MODE=full # 'full' or 'lazy'

# E2E
E2E_API_PORT=3001
E2E_WEB_PORT=5173